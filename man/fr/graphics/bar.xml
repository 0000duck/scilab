<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>fr</LANGUAGE>

  <TITLE>bar</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>2005</DATE>

  <SHORT_DESCRIPTION name="bar">dessine un histogramme de type 'bar' a la
  Matlab</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>bar(x,y ,[width])</CALLING_SEQUENCE_ITEM>

    <CALLING_SEQUENCE_ITEM>bar(x,Y,[width])</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>x</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: vecteur ligne ou colonne de taille N.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>

      <PARAM_ITEM>
        <PARAM_NAME>y</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: vecteur ligne ou colonne de taille N.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>

      <PARAM_ITEM>
        <PARAM_NAME>Y</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: matrice de taille MxN.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>

      <PARAM_ITEM>
        <PARAM_NAME>width</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: (optionnel) définit la largeur (en pourcentage de la taille
          disponible) pour la barre.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>A FAIRE</P>
  </DESCRIPTION>

  <EXAMPLE></EXAMPLE>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>plot</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>surf</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHOR>Farid Belacehne</AUTHOR>
</MAN>