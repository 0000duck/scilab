<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>bar</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>2005</DATE>

  <SHORT_DESCRIPTION name="bar">bar histogram</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>bar(x,y ,[width])</CALLING_SEQUENCE_ITEM>

    <CALLING_SEQUENCE_ITEM>bar(x,Y,[width])</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>x</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: line or column vector of size N.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>

      <PARAM_ITEM>
        <PARAM_NAME>y</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: line or column vector of size N.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>

      <PARAM_ITEM>
        <PARAM_NAME>Y</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: MxN matrix.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>

      <PARAM_ITEM>
        <PARAM_NAME>width</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: (optional) defines the width (a percentage of the available
          room) for the bar.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>TO DO</P>
  </DESCRIPTION>

  <EXAMPLE></EXAMPLE>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>plot</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>surf</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHOR>Farid Belacehne</AUTHOR>
</MAN>