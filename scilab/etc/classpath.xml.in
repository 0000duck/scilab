<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE classpaths SYSTEM "../modules/jvm/xml/classpath.dtd">
<!-- =================== -->
<!--
 default CLASSPATH for Scilab

 @author Allan CORNET
 @author Sylvestre LEDRU
 @date INRIA 2007/2008
 ===================
 Don't touch if you do not know what you are doing
-->
<!-- =================== -->
<!-- 
load can be:
* background
* onUse
* startup
startup is the default
-->

<classpaths>
<path value="$SCILAB/thirdparty/"/>

<!-- Mandatory on startup -->
<path value="@FLEXDOCK@"/>
<path value="@JROSETTA_API@"/>
<path value="@JROSETTA_ENGINE@"/>
<path value="@SKINLF@"/>
<path value="@LOOKS@"/>
<path value="@COMMONS_LOGGING@"/>
<path value="@JHALL@"/>

<!-- Load when needed -->
<path value="@JOGL@" load="onUse">
	<load on="graphics"/>
	<load on="graphic_export"/>
</path>
<path value="@GLUEGEN_RT@" load="onUse">
	<load on="graphics"/>
	<load on="graphic_export"/>
</path>

<!-- Documentation (not mandatory) -->
<path value="@JEUCLID_CORE@" load="onUse">
	<load on="documentationGeneration"/>
</path>
<path value="@FOP@" load="onUse">
	<load on="documentationGeneration"/>
</path>
<path value="@SAXON@" load="onUse">
	<load on="documentationGeneration"/>
</path>
<path value="@BATIK@" load="onUse">
	<load on="documentationGeneration"/>
</path>

<path value="$SCILAB/modules/jvm/jar/org.scilab.modules.jvm.jar"/>
<path value="$SCILAB/modules/shell/jar/org.scilab.modules.shell.jar"/>
<path value="$SCILAB/modules/history_manager/jar/org.scilab.modules.history_manager.jar"/>
<path value="$SCILAB/modules/action_binding/jar/org.scilab.modules.action_binding.jar"/>
<path value="$SCILAB/modules/console/jar/org.scilab.modules.console.jar"/>
<path value="$SCILAB/modules/gui/jar/org.scilab.modules.gui.jar"/>
<path value="$SCILAB/modules/gui/etc"/> <!-- Temporary fix for Flexdock using GTK theme -->
<path value="$SCILAB/modules/javasci/jar/javasci.jar"/>
<path value="$SCILAB/modules/renderer/jar/org.scilab.modules.renderer.jar"/>
<path value="$SCILAB/modules/localization/jar/org.scilab.modules.localization.jar"/>
<path value="$SCILAB/modules/completion/jar/org.scilab.modules.completion.jar"/>
<path value="$SCILAB/modules/graphic_export/jar/org.scilab.modules.graphic_export.jar"/>
<path value="$SCILAB/modules/helptools/jar/org.scilab.modules.helptools.jar"/>
</classpaths>
