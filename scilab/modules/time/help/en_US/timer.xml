<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
	<LANGUAGE>eng</LANGUAGE>
	<TITLE>timer</TITLE>
	<TYPE>Scilab Function</TYPE>
 <DATE>$LastChangedDate$</DATE>
	<SHORT_DESCRIPTION name="timer">cpu time</SHORT_DESCRIPTION>
	
	<CALLING_SEQUENCE>
		<CALLING_SEQUENCE_ITEM>timer()</CALLING_SEQUENCE_ITEM>
	</CALLING_SEQUENCE>
	
	<DESCRIPTION>
		<P>Returns the CPU time from the preceding call to <VERB>timer()</VERB>.</P>
		<P>timer has a time precision of 100 nanoseconds.</P>
	</DESCRIPTION>
	
	<EXAMPLE><![CDATA[
timer();A=rand(100,100);timer()
	]]></EXAMPLE>
	
	<SEE_ALSO>
		<SEE_ALSO_ITEM><LINK>getdate</LINK></SEE_ALSO_ITEM>
		<SEE_ALSO_ITEM><LINK>toc</LINK></SEE_ALSO_ITEM>
		<SEE_ALSO_ITEM><LINK>tic</LINK></SEE_ALSO_ITEM>
		<SEE_ALSO_ITEM><LINK>etime</LINK></SEE_ALSO_ITEM>
	</SEE_ALSO>
</MAN>
