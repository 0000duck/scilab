<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
	<LANGUAGE>eng</LANGUAGE>
	<TITLE>eomday</TITLE>
	<TYPE>Scilab Function</TYPE>
	<DATE>$LastChangedDate$</DATE>
	
	<SHORT_DESCRIPTION name="eomday">Return last day of month</SHORT_DESCRIPTION>
	
	<CALLING_SEQUENCE>
		<CALLING_SEQUENCE_ITEM>E = eomday(Y, M)</CALLING_SEQUENCE_ITEM>
	</CALLING_SEQUENCE>
	
	<DESCRIPTION>
		<P>E = eomday(Y, M) returns the last day of the year and month given by corresponding elements of arrays Y and M.</P>
	</DESCRIPTION>
	
	<EXAMPLE>
<![CDATA[
eomday(2006,3);
]]>
	</EXAMPLE>
	
	<SEE_ALSO>
		<SEE_ALSO_ITEM>
			<LINK>datenum</LINK>
		</SEE_ALSO_ITEM>
		
		<SEE_ALSO_ITEM>
			<LINK>datevec</LINK>
		</SEE_ALSO_ITEM>
		
		<SEE_ALSO_ITEM>
			<LINK>weekday</LINK>
		</SEE_ALSO_ITEM>
	</SEE_ALSO>
	
	<AUTHORS>
		<AUTHORS_ITEM>P.M</AUTHORS_ITEM>
	</AUTHORS>
</MAN>
