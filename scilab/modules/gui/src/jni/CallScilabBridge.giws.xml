<package name="org.scilab.modules.gui.bridge">
  <object name="CallScilabBridge">
<!-- Object creation -->
	<method name="newWindow" returnType="int" modifier="static"/>
	<method name="newMenuBar" returnType="int" modifier="static"/>
	<method name="newMenu" returnType="int" modifier="static"/>        
	<method name="newPushButton" returnType="int" modifier="static"/>        
	<method name="newEditBox" returnType="int" modifier="static"/>        
	<method name="newLabel" returnType="int" modifier="static"/>        
	<method name="newCheckBox" returnType="int" modifier="static"/>        
	<method name="newRadioButton" returnType="int" modifier="static"/>      	<method name="newSlider" returnType="int" modifier="static"/>        
<!-- Parent setting -->
	<method name="setFigureAsParent" returnType="void" modifier="static">
	  <parameter type="int" name="figureID"/>
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setMenuAsParent" returnType="void" modifier="static">
	  <parameter type="int" name="menuID"/>
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setRootAsParent" returnType="void" modifier="static">
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setParent" returnType="void" modifier="static">
	  <parameter type="int" name="parentID"/>
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setPushButtonParent" returnType="void" modifier="static">
	  <parameter type="int" name="parentID"/>
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setEditBoxParent" returnType="void" modifier="static">
	  <parameter type="int" name="parentID"/>
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setLabelParent" returnType="void" modifier="static">
	  <parameter type="int" name="parentID"/>
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setCheckBoxParent" returnType="void" modifier="static">
	  <parameter type="int" name="parentID"/>
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setRadioButtonParent" returnType="void" modifier="static">
	  <parameter type="int" name="parentID"/>
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setSliderParent" returnType="void" modifier="static">
	  <parameter type="int" name="parentID"/>
	  <parameter type="int" name="objID"/>
	</method>
<!-- Text setting -->
	<method name="setWidgetText" returnType="void" modifier="static">
	  <parameter type="int" name="objID"/>
	  <parameter type="String" name="text"/>
	</method>
	<method name="getWidgetText" returnType="String" modifier="static">
	  <parameter type="int" name="objID"/>
	</method>
<!-- Color setting -->
	<method name="setWidgetBackgroundColor" returnType="void" modifier="static">
	  <parameter type="int" name="objID"/>
	  <parameter type="int" name="red"/>
	  <parameter type="int" name="green"/>
	  <parameter type="int" name="blue"/>
	</method>
	<method name="getWidgetBackgroundColor" returnType="int[]" modifier="static">
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setWidgetForegroundColor" returnType="void" modifier="static">
	  <parameter type="int" name="objID"/>
	  <parameter type="int" name="red"/>
	  <parameter type="int" name="green"/>
	  <parameter type="int" name="blue"/>
	</method>
	<method name="getWidgetForegroundColor" returnType="int[]" modifier="static">
	  <parameter type="int" name="objID"/>
	</method>
<!-- Font setting -->
	<method name="setWidgetFontWeight" returnType="void" modifier="static">
	  <parameter type="int" name="objID"/>
	  <parameter type="String" name="weight"/>
	</method>
	<method name="setWidgetFontSize" returnType="void" modifier="static">
	  <parameter type="int" name="objID"/>
	  <parameter type="int" name="size"/>
	</method>
	<method name="getWidgetFontSize" returnType="int" modifier="static">
	  <parameter type="int" name="objID"/>
	</method>
	<method name="setWidgetFontAngle" returnType="void" modifier="static">
	  <parameter type="int" name="objID"/>
	  <parameter type="String" name="angle"/>
	</method>
<!-- Position setting -->
	<method name="setWidgetPosition" returnType="void" modifier="static">
	  <parameter type="int" name="objID"/>
	  <parameter type="int" name="x"/>
	  <parameter type="int" name="y"/>
	  <parameter type="int" name="width"/>
	  <parameter type="int" name="height"/>
	</method>
	<method name="getWidgetPosition" returnType="int[]" modifier="static">
	  <parameter type="int" name="objID"/>
	</method>
<!-- Callback setting -->
	<method name="setWidgetCallback" returnType="void" modifier="static">
	  <parameter type="int" name="objID"/>
	  <parameter type="String" name="text"/>
	</method>
	<method name="getWidgetCallback" returnType="String" modifier="static">
	  <parameter type="int" name="objID"/>
	</method>
<!-- Enable/Disable -->
	<method name="setRootMenuEnabled" returnType="void" modifier="static">
	  <parameter type="String" name="menuName"/>
	  <parameter type="boolean" name="status"/>
	</method>
	<method name="setFigureMenuEnabled" returnType="void" modifier="static">
	  <parameter type="int" name="figureID"/>
	  <parameter type="String" name="menuName"/>
	  <parameter type="boolean" name="status"/>
	</method>
<!-- Delete -->
	<method name="removeRootMenu" returnType="void" modifier="static">
	  <parameter type="String" name="menuName"/>
	</method>
	<method name="removeFigureMenu" returnType="void" modifier="static">
	  <parameter type="int" name="figureID"/>
	  <parameter type="String" name="menuName"/>
	</method>
<!-- File chooser -->
	<method name="newFileChooser" returnType="int" modifier="static"/>      
	<method name="setFileChooserTitle" returnType="void" modifier="static">
            <parameter name="id" type="int"/>
            <parameter name="title" type="String"/>
        </method>      
	<method name="setFileChooserInitialDirectory" returnType="void" modifier="static">
            <parameter name="id" type="int"/>
            <parameter name="path" type="String"/>
        </method>      
	<method name="setFileChooserMask" returnType="void" modifier="static">
            <parameter name="id" type="int"/>
            <parameter name="mask" type="String"/>
        </method>      
	<method name="fileChooserDisplayAndWait" returnType="void" modifier="static">
            <parameter name="id" type="int"/>
        </method>      
	<method name="getFileChooserSelectionSize" returnType="int" modifier="static">
            <parameter name="id" type="int"/>
        </method>      
	<method name="getFileChooserSelection" returnType="String[]" modifier="static">
            <parameter name="id" type="int"/>
        </method>      
	<method name="setFileChooserDirectorySelectionOnly" returnType="void" modifier="static">
            <parameter name="id" type="int"/>
        </method>      
	<method name="setFileChooserFileSelectionOnly" returnType="void" modifier="static">
            <parameter name="id" type="int"/>
        </method>      
  </object>
</package>