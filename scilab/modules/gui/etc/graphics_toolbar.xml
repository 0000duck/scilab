<?xml version="1.0"?>
<toolbar>
<!-- [SCILAB_FIGURE_ID] pattern is replaced at generation of Java menu by the ID of the figure --> 
    <!-- Rotate -->
    <button icon="view-refresh.png" enabled="false"/>

    <separator/>

    <!-- Zoom In -->
    <button icon="list-add.png" enabled="false"/>
    <!-- Zoom Out -->
    <button icon="list-remove.png" enabled="false"/>

    <separator/>

    <!-- Help -->
    <button icon="help-browser.png">
        <callback instruction="help();" type="0"/>
    </button>
</toolbar>