<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>halt</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate: 2006-09-10 20:58:00 +0200 (dim., 10 sept. 2006)
  $</DATE>

  <SHORT_DESCRIPTION name="halt">stop execution</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>halt()</CALLING_SEQUENCE_ITEM>

    <CALLING_SEQUENCE_ITEM>halt('a message')</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <DESCRIPTION>
    <P>stops execution until something is entered in the keyboard.</P>
  </DESCRIPTION>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>pause</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>return</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>exec</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>