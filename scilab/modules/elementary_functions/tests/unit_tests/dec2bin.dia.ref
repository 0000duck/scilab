// =============================================================================
if dec2bin(0) <> "0"  then bugmes();quit;end
if dec2bin(1) <> "1"  then bugmes();quit;end
if dec2bin(2) <> "10" then bugmes();quit;end
if dec2bin(3) <> "11" then bugmes();quit;end
A = [1 2 ; 3 4];
B = [ "001" ; "011" ; "010" ; "100" ];
if dec2bin(A) <> B then bugmes();quit;end
A = floor(rand(2500,1) * 10^9);
if or(bin2dec(dec2bin(A)) <> A ) then bugmes();quit;end
