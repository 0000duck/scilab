<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>dec2oct</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>$LastChangedDate: 2006-09-10 20:58:00 +0200 (Sun, 10 Sep 2006) $</DATE>
  <SHORT_DESCRIPTION name="dec2oct">octal representation of integers</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>o=dec2oct(d)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>d</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: matrix of non negative integers</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      <PARAM_ITEM>
        <PARAM_NAME>o</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: matrix of character strings</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P><VERB>dec2oct(x)</VERB> returns the octal representation of a matrix
    of integers</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
dec2oct([2748 10;11 3])
]]>
  </EXAMPLE>
