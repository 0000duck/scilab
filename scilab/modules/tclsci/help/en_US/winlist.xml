<SHORT_DESCRIPTION name="winlist"> Return the winId of current window created by sciGUI</SHORT_DESCRIPTION>

<CALLING_SEQUENCE>
<CALLING_SEQUENCE_ITEM>winIds=winlist()</CALLING_SEQUENCE_ITEM>
</CALLING_SEQUENCE>

<PARAM>
<PARAM_INDENT>
