<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>javaclasspath</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate: 2007-05-10 09:26:08 +0200$</DATE>

  <SHORT_DESCRIPTION name="javaclasspath">set and get dynamic Java class path</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>res=javaclasspath()</CALLING_SEQUENCE_ITEM>

    <CALLING_SEQUENCE_ITEM>javaclasspath(path)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>res</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a string matrix</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>set and get the dynamic Java path to one or more directory or file
    specifications given in path.</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[res=javaclasspath();
javaclasspath(SCI);
javaclasspath([SCI,SCI+'/java']);

]]></EXAMPLE>

  <AUTHOR>A.C</AUTHOR>
</MAN>
