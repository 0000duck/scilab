  <SHORT_DESCRIPTION name="with_embedded_jre">checks if scilab uses a embedded JRE</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>res=with_embedded_jre()</CALLING_SEQUENCE_ITEM>
