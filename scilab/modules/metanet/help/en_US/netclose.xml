<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>netclose</TITLE>
  <TYPE> Scilab Function</TYPE>
  <DATE>$LastChangedDate$</DATE>
  <SHORT_DESCRIPTION name="netclose"> closes an <VERB>edit_graph</VERB> window</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>netclose(window)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>window</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: integer, window number</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P>
    Each <VERB>editgraph</VERB> window has a window number returned by <VERB>edit_graph</VERB>
    and <VERB>show_graph</VERB> functions. This function is used to
    close the <VERB>edit_graph</VERB> window with number <VERB>window</VERB>.</P>
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>edit_graph</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>netwindow</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>netwindows</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>show_graph</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>
