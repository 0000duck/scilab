<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>metanet_module_path</TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>$LastChangedDate$</DATE>
  <SHORT_DESCRIPTION name="metanet_module_path">Returns the path of the metanet module</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>p = metanet_module_path()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
  <PARAM_INDENT>

    <PARAM_ITEM>
    <PARAM_NAME>p</PARAM_NAME>
    <PARAM_DESCRIPTION>
       <SP>
       : a string, The module path.
       </SP>
    </PARAM_DESCRIPTION>
    </PARAM_ITEM>
  </PARAM_INDENT>
  </PARAM>
 
  <DESCRIPTION>
     <P><VERB>p=metanet_module_path()</VERB> return the path of the
     metanet module. This path may be useful to retreive Metanet data
     files.</P>

  </DESCRIPTION>

  <EXAMPLE><![CDATA[
  g=load_graph(metanet_module_path()+'/demos/mesh100.graph');
  show_graph(g);
  ]]></EXAMPLE>

 
</MAN>
