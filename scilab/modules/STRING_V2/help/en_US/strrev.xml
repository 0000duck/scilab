<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>strrev</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>$LastChangedDate: 2006-08-08 16:57:13 +0200 (Tue, 08 Aug 2006) $</DATE>
  <SHORT_DESCRIPTION name="strrev">returns string reversed</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM> res = strrev(str1)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>str1</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: string or matrix of strings</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      
      <PARAM_ITEM>
        <PARAM_NAME>res</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: string or matrix of strings</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P><VERB>res = strrev(str1)</VERB> Returns string reversed.</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
rev = strrev('This is a simple string')
strrev(rev)
strrev(['This is a simple string','scilab'])
 ]]>
  </EXAMPLE>
</MAN>
