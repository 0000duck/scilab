<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>gce</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate$</DATE>

  <SHORT_DESCRIPTION name="gce">Get current entity handle.</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>e = gce()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>e</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: handle, the handle of the current entity.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>This routine returns the handle of the last created (and still
    existent) entity.</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[
   
    a=gca() //get the handle of the newly created axes
    a.data_bounds=[1,1;10,10];
    a.axes_visible = 'on' ;
    for i=1:5
      xfrect(7-i,9-i,3,3);
      e=gce();
      e.background=i;
     end
    delete(); // delete current entity
    delete(gce()); // delete current entity
    delete(gcf()); // delete current figure
     

  ]]></EXAMPLE>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>gcf</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>gca</LINK>
    </SEE_ALSO_ITEM>

