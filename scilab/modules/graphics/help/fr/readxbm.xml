<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>readxbm</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>$LastChangedDate$</DATE>
  <SHORT_DESCRIPTION name="readxbm"> read a xbm graphic file</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>[cmap,pixels]=readxbm(filen)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
    
      <PARAM_ITEM>
        <PARAM_NAME>cmap</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: colormap matrix.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      
      <PARAM_ITEM>
        <PARAM_NAME>pixels</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: pixels matrix.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      
      <PARAM_ITEM>
        <PARAM_NAME>filen</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: string, a xbm filename.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      
      
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P><VERB>readxbm</VERB> read a xbm graphic file.</P>
  </DESCRIPTION>
  <EXAMPLE><![CDATA[ 
    [cmap,pixels]=readxbm('test.xbm')
  ]]></EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>readgif</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>
