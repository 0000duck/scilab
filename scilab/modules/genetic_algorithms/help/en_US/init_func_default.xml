<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>init_func_default</TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>4-Feb-2008</DATE>
  <SHORT_DESCRIPTION name="init_func_default"> A function a initialize a population </SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>Pop_init = init_func_default(popsize,param)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
  <PARAM_INDENT>

    <PARAM_ITEM>
    <PARAM_NAME>popsize</PARAM_NAME>
    <PARAM_DESCRIPTION>
       <SP>
       : the number of individuals to generate.
       </SP>
    </PARAM_DESCRIPTION>
    </PARAM_ITEM>

    <PARAM_ITEM>
    <PARAM_NAME>param</PARAM_NAME>
    <PARAM_DESCRIPTION>
       <SP>
       : a list of parameters.
         - 'dimension': the size of the vector X.
         - 'minbound': a vector of minimum bounds for the variable X.
         - 'maxbound': a vector of maximum bounds for the variable X.
       </SP>
    </PARAM_DESCRIPTION>
    </PARAM_ITEM>

    <PARAM_ITEM>
    <PARAM_NAME>Pop_init</PARAM_NAME>
    <PARAM_DESCRIPTION>
       <SP>
       : a list which contains the initial population of individuals.
       </SP>
    </PARAM_DESCRIPTION>
    </PARAM_ITEM>
  </PARAM_INDENT>
  </PARAM>
 
  <DESCRIPTION>
     <DESCRIPTION_INDENT>
     <DESCRIPTION_ITEM>
     <P>
      This function generate an initial population containing pop_size individuals.
     </P>
     </DESCRIPTION_ITEM>
     </DESCRIPTION_INDENT>
  </DESCRIPTION>

  <SEE_ALSO>
