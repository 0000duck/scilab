// <-- TEST WITH GRAPHIC -->
// =============================================================================
// Tests for pca function
// =============================================================================
if execstr('pca()','errcatch')==0 then pause,end
// =============================================================================
a=rand(100,10,'n');
[r1,r2,r3] = pca(a);
// =============================================================================
[ra,rb,rc] = pca(a, [1 2]);
if ( or(r1 <> ra) | or(r2 <> rb) | or(r3 <> rc)) then pause,end
// =============================================================================
[ra,rb,rc] = pca(a, [1 2],%t);
if ( or(r1 <> ra) | or(r2 <> rb) | or(r3 <> rc)) then pause,end
// =============================================================================
[ra,rb,rc] = pca(a, [1 2],%f);
if ( or(r1 <> ra) | or(r2 <> rb) | or(r3 <> rc) ) then pause,end
// =============================================================================
