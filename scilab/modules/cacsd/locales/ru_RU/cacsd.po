#, c-format
msgid "Function %s is obsolete."
msgstr "Функция %s является устаревшей."

#, c-format
msgid "Please use %s instead."
msgstr "Пожалуйста используйте %s вместо этого."

#, c-format
msgid "This function will be permanently removed in Scilab %s"
msgstr "Эта функция будет навсегда удалена из Scilab %s"

#, c-format
msgid "%s: There is no file named %s.sci in the library directory %s.\n"
msgstr "%s: Файл с именем %s.sci не существует в каталоге библиотеки %s.\n"

#, c-format
msgid "Checking: %s.\n"
msgstr "Проверка: %s.\n"

#, c-format
msgid "%s: Please check: %s\n"
msgstr "%s: Пожалуйста проверьте: %s\n"

#, c-format
msgid "%s: File %s doesn''t exist.\n"
msgstr "%s: Файл %s не существует.\n"

#, c-format
msgid "%s: No files with extension %s found in %s\n"
msgstr "%s: Файлы с расширением %s не найдены в %s\n"

#, c-format
msgid "%s: %s file compilation forced\n"
msgstr "%s: Вызвана компиляция файла %s\n"

#, c-format
msgid "%s: Processing file: %s\n"
msgstr "%s: Обработка файла: %s\n"

#, c-format
msgid "%s: Regenerate names and lib\n"
msgstr "%s: Регенерация имен и библиотек\n"

#, c-format
msgid "%s: %s file cannot be created\n"
msgstr "%s: %s файл не может быть создан\n"

#, c-format
msgid ""
"Library file %s has been updated,\n"
"but cannot be loaded into Scilab because ''%s'' is a protected variable.\n"
msgstr ""
"Библиотечный файл %s был обновлен,\n"
"но не может быть загружен в Scilab потому что ''%s'' - защищенная "
"переменная.\n"

#, c-format
msgid "%s: Warning: Error in file %s : %s. File ignored\n"
msgstr "%s: Предупреждение: Ошибка в файле %s: %s. Файл проигнорирован\n"

#, c-format
msgid "%s: Impossible to open file %s for writing\n"
msgstr "%s: Невозможно открыть файл %s для записи\n"

#, c-format
msgid "%s: File %s does not contain any function.\n"
msgstr "%s: Файл %s не содержит функции.\n"

#, c-format
msgid "%s: The function has not been built for profiling"
msgstr "%s: Функция не была создана для профилирования"

#, c-format
msgid ""
"%s: Wrong values for input argument: No profile data in the function\n"
msgstr ""
"%s: Неверные значения для введенного аргумента: Отсутствуют данные профиля в "
"функции\n"

msgid "Complexity"
msgstr "Сложность"

msgid "3D Rot."
msgstr "3D Вращ."

msgid "Edit"
msgstr "Редактировать"

msgid "Editer"
msgstr "Редактор"

msgid "Insert"
msgstr "Вставить"

msgid "3D &Rot."
msgstr "3D &Вращ."

msgid "&Edit"
msgstr "&Изменить"

msgid "&Insert"
msgstr "&Вставить"

msgid "Exit"
msgstr "Выход"

msgid "Click to get corresponding line, move with a-z."
msgstr ""
"Щелкните, чтобы получить соответствующую строку, a-z для перемещения."

#, c-format
msgid "line %s [%s call, %s sec] :: "
msgstr "строка %s [%s вызов, %s сек] :: "

#, c-format
msgid "line %s [%s calls, %s sec] :: "
msgstr "строка %s [%s вызовов, %s сек] :: "

msgid "UnZoom"
msgstr "Уменьшить"

msgid "Zoom"
msgstr "Увеличить"

msgid "File"
msgstr "Файл"

msgid "&File"
msgstr "&Файл"

msgid "&Tools"
msgstr "&Инструменты"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: Неверный тип аргумента #%d.\n"

#, c-format
msgid "%s: I cannot find any files with extension %s in %s\n"
msgstr "%s: Не могу найти файл с расширением %s в %s\n"

#, c-format
msgid "%s: Incorrect function in file %s.\n"
msgstr "%s: Неверная функция в файле %s.\n"

#, c-format
msgid "-- Creation of [%s] (Macros) --\n"
msgstr "-- Создание [%s] (Макрос) --\n"

#, c-format
msgid "%s: obsolete op-code %d.\n"
msgstr "%s: устаревший код операции %d.\n"

#, c-format
msgid "No variable named: %s"
msgstr "Нет переменной с именем: %s"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: A Scilab function name is expected.\n"
msgstr ""
"%s: Неверное значение для введенного аргумента #%d: Ожидается имя функции "
"Scilab.\n"

#, c-format
msgid "%s is already noncompiled, nothing to do!"
msgstr "%s уже не собран, действие не выполнено!"

#, c-format
msgid "%s is already compiled, nothing to do!"
msgstr "%s уже не собран, действие не выполнено!"

#, c-format
msgid "%s is already compiled for profiling, nothing to do!"
msgstr "%s уже собран для профилирования, действие не выполнено!"

#, c-format
msgid "%s: Wrong type for input argument #%d: A character string expected.\n"
msgstr ""
"%s: Неверный тип для введенного аргумента #%d: Ожидается строка символов.\n"

#, c-format
msgid "%s: Undefined variable %s.\n"
msgstr "%s: Неопределенная переменная %s.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Name of a Scilab function expected.\n"
msgstr ""
"%s: Неверное значение введенного аргумента #%d: Ожидается имя функции "
"Scilab.\n"

msgid "No comment available.\n"
msgstr "Комментарий отсутствует.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Scilab function expected.\n"
msgstr ""
"%s:  Неверный тип для введенного аргумента #%d: Ожидается функция Scilab.\n"
