<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>gethistory</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate$</DATE>

  <SHORT_DESCRIPTION name="gethistory">returns current scilab history in a
  string matrix </SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>matstr=gethistory()</CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>line=gethistory(N)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

	<PARAM>
		<PARAM_INDENT>

			<PARAM_ITEM>
				<PARAM_NAME>matstr</PARAM_NAME>
				<PARAM_DESCRIPTION>
				<P>a string matrix</P>
				</PARAM_DESCRIPTION>
			</PARAM_ITEM>

			<PARAM_ITEM>
				<PARAM_NAME>N</PARAM_NAME>
				<PARAM_DESCRIPTION>
				<P>Nth line in scilab's history</P>
				</PARAM_DESCRIPTION>
			</PARAM_ITEM>

			<PARAM_ITEM>
				<PARAM_NAME>line</PARAM_NAME>
				<PARAM_DESCRIPTION>
				<P>a string</P>
				</PARAM_DESCRIPTION>
			</PARAM_ITEM>

		</PARAM_INDENT>
	</PARAM>

  <DESCRIPTION>
    <P>returns current scilab history in a string matrix.</P>
  </DESCRIPTION>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>savehistory</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>loadhistory</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>resethistory</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHORS>
    <AUTHORS_ITEM>A.C</AUTHORS_ITEM>
  </AUTHORS>
</MAN>
