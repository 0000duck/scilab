<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>sethistoryfile</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate: 2006-08-17 18:06:16 +0200 (jeu., 17 ao√ªt 2006) $</DATE>

  <SHORT_DESCRIPTION name="sethistoryfile">set filename for scilab history</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>sethistoryfile(filename)</CALLING_SEQUENCE_ITEM>

    <CALLING_SEQUENCE_ITEM>sethistoryfile()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>filename</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: filename for history</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>set filename for scilab history.</P>

    <P>sethistoryfile() without parameters will use the default filename
    (SCIHOME/history.scilab)</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[gethistoryfile()
	sethistoryfile(gethistoryfile())

]]></EXAMPLE>

  <AUTHORS>
    <AUTHORS_ITEM>A.C</AUTHORS_ITEM>
  </AUTHORS>
</MAN>