<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>addhistory</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate: 2006-08-30 13:26:08 +0200 (mer., 30 ao√ªt 2006)$</DATE>

  <SHORT_DESCRIPTION name="addhistory">add lines to current history.</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>addhistory(string)</CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>addhistory(string_matrix)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

	<PARAM>
		<PARAM_INDENT>
			
			<PARAM_ITEM>
				<PARAM_NAME>string</PARAM_NAME>
				<PARAM_DESCRIPTION>
				<SP>: a string</SP>
				</PARAM_DESCRIPTION>
			</PARAM_ITEM>
			
			<PARAM_ITEM>
				<PARAM_NAME>string_matrix</PARAM_NAME>
				
				<PARAM_DESCRIPTION>
				<SP>: a string matrix</SP>
				</PARAM_DESCRIPTION>
			</PARAM_ITEM>
		
		</PARAM_INDENT>
	</PARAM>

  <DESCRIPTION>
    <P>add lines to current history.</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[addhistory('hello') 
addhistory(['hello','Scilab')]]></EXAMPLE>

  <AUTHORS>
    <AUTHORS_ITEM>A.C</AUTHORS_ITEM>
  </AUTHORS>
</MAN>
