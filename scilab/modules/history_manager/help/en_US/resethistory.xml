  <TITLE>resethistory</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate$</DATE>

  <SHORT_DESCRIPTION name="resethistory">Deletes all entries in the scilab history.</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
