<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>historymanager</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate: 2006-08-17 18:06:16 +0200 (jeu., 17 ao√ªt 2006)$</DATE>

  <SHORT_DESCRIPTION name="historymanager">enable or disable history manager</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>state1=historymanager(state2)</CALLING_SEQUENCE_ITEM>

    <CALLING_SEQUENCE_ITEM>state1=historymanager()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>state1</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: returns history manager state 'on' or 'off'</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>

      <PARAM_ITEM>
        <PARAM_NAME>state2</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: 'on' or 'off' set history manager's state</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>enable or disable history manager.</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[displayhistory()
backupstate=historymanager()
historymanager('off')
displayhistory()
historymanager('on')
loadhistory()
displayhistory()
historymanager(backupstate)


]]></EXAMPLE>

  <AUTHOR>A.C</AUTHOR>
</MAN>