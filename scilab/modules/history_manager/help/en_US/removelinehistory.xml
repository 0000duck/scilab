<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>removelinehistory</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate: 2006-08-17 18:06:16 +0200 (jeu., 17 ao√ªt 2006)
  $</DATE>

  <SHORT_DESCRIPTION name="removelinehistory">remove the Nth line in history.</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>removelinehistory(N)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>N</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a line number</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>remove the Nth line in history.</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[
  displayhistory()
  removelinehistory(historysize()-2)
  displayhistory()

]]></EXAMPLE>

  <AUTHORS>
    <AUTHORS_ITEM>A.C</AUTHORS_ITEM>
  </AUTHORS>
</MAN>