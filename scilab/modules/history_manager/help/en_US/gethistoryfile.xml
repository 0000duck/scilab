  <SHORT_DESCRIPTION name="gethistoryfile">get filename used for scilab's history</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>filename = gethistoryfile()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>filename</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: file name used for history</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
		</PARAM_INDENT>
  </PARAM>


  <DESCRIPTION>
    <P>get filename for scilab's history</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[gethistoryfile()
	gethistoryfile()

]]></EXAMPLE>

  <AUTHORS>
