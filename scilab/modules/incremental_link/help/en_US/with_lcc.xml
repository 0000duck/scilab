<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>with_lcc</TITLE>

  <TYPE>Scilab Function</TYPE>


  <SHORT_DESCRIPTION name="with_lcc">returns if LCC-Win32 is the default C Compiler.</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>bOK=with_lcc()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>bOK</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: returns %Tif LCC-Win32 is the default C Compiler. </SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>returns if LCC-Win32 is the default C Compiler.</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[bOK=with_lcc()
]]></EXAMPLE>

  <AUTHOR>Allan CORNET</AUTHOR>
</MAN>
