<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>getmodules</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate: 2006-12-12 09:26:08 +0200$</DATE>

  <SHORT_DESCRIPTION name="getmodules">returns list of modules installed in
  Scilab</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>res=getmodules()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>res</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a string matrix</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>Returns list of modules installed in Scilab.</P>
  </DESCRIPTION>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>with_module</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHOR>A.c</AUTHOR>
</MAN>