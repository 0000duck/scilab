<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>with_macros_source</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>2006</DATE>

  <SHORT_DESCRIPTION name="with_macros_source">Checks if macros source are installed</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>r=with_macros_source()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>r</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a boolean</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>Returns <VERB>%t</VERB> if macros source are installed or
    <VERB>%f</VERB> if not.</P>
  </DESCRIPTION>
</MAN>
