<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>getmd5</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>$LastChangedDate$</DATE>

  <SHORT_DESCRIPTION name="getmd5">get md5 checksum</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>res=getmd5(filename)</CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>res=getmd5(ParamString,'string')</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>res</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: md5 result (a string)</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      <PARAM_ITEM>
        <PARAM_NAME>filename</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: filename (a string)</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      <PARAM_ITEM>
        <PARAM_NAME>ParamString</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: a string</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  
  <DESCRIPTION>
    <P><VERB>getmd5(...)</VERB> get md5 checksum of a file or a string.</P>
  </DESCRIPTION>


  <AUTHORS>
    <AUTHORS_ITEM>A.C.</AUTHORS_ITEM>
  </AUTHORS>
</MAN>
