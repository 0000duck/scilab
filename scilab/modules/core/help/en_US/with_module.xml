<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>with_module</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate: 2006-12-12 09:26:08 +0200$</DATE>

  <SHORT_DESCRIPTION name="with_module">Checks if a Scilab module is
  installed</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>r=with_module(module_name)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>r</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a boolean</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>

    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>module_name</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a string. example : 'core'</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>Returns <VERB>%t</VERB> Checks if a Scilab module is installed.</P>
  </DESCRIPTION>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>getmodules</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHOR>A.C</AUTHOR>
</MAN>