<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>libraryinfo</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate: 2007-07-29 20:39:36 +0200 (mar., 17 oct. 2007)$</DATE>

  <SHORT_DESCRIPTION name="libraryinfo">get macros and path of a scilab library</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>macros = libraryinfo(libraryname)</CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>[macros,path] = libraryinfo(libraryname)</CALLING_SEQUENCE_ITEM>

  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>macros</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a string matrix (all macros of the library)</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>path</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a string (path of library)</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
    
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>libraryname</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a string (library name)</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>

  </PARAM>

  <DESCRIPTION>
    <P>get macros and path of a scilab library.</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[[m,p]=libraryinfo('corelib')
   ]]></EXAMPLE>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>librarieslist</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>
