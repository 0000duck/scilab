#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: ein Fehler ist aufgetreten: %s\n"

#, c-format
msgid "%s: work space (stacksize) is too small d\n"
msgstr "%s: Arbeistbereich (Stackgröße) ist zu klein d.\n"

#, c-format
msgid "%s: Unknow type or not yet implemented\n"
msgstr "%s: unbekannter Typ oder noch nicht implementiert\n"

#, c-format
msgid "%s: Wrong first input argument: '%s' or '%s' expected.\n"
msgstr "%s: falsches erstes Eingangsargument: '%s' oder '%s' erwartet.\n"

msgid "Scilab PVM module not installed.\n"
msgstr "Scilab PVM Modul nicht installiert.\n"

msgid "Unknown Error"
msgstr "unbekannter Fehler"

msgid "OK"
msgstr "OK"

msgid "Bad parameter"
msgstr "falscher Parameter"

msgid "Barrier count mismatch"
msgstr "Fehler beim Zählen der Grenze"

msgid "Read past end of buffer"
msgstr "Lese letztes Ende des Buffers"

msgid "No such host"
msgstr "Kein solcher Rechner"

msgid "No such executable"
msgstr "kein derartige ausführbare Datei"

msgid "Can not get memory"
msgstr "Kann Speicher nicht belegen"

msgid "Can not decode received msg"
msgstr "kann eigende Nachricht nicht dekodieren"

msgid "Daemon pvmd is not responding"
msgstr "Hintergrundprozess pvmd reagiert nicht."

