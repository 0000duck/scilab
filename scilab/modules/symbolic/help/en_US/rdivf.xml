<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>ldivf</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>$LastChangedDate$</DATE>
  <SHORT_DESCRIPTION name="rdivf"> right symbolic division</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>[&quot;r&quot;]=ldivf(&quot;d&quot;,&quot;c&quot;)   </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>&quot;d&quot;,&quot;c&quot;,&quot;r&quot;  </PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: strings</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P>
    returns the string <VERB>&quot;c/d&quot;</VERB>
    Trivial simplifications such as <VERB>&quot;c/1&quot; = &quot;c&quot;</VERB> are performed.</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
ldivf('c','d')
ldivf('1','2')
ldivf('a','0')
 ]]>
  </EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>ldivf</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>
