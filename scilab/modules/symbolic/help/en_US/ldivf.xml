<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>ldivf</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>$LastChangedDate$</DATE>
  <SHORT_DESCRIPTION name="ldivf"> left symbolic division</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>ldivf('d','c')   </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <DESCRIPTION>
    <P>
    returns the string <VERB>'c\d'</VERB>
    Trivial simplifications such as <VERB>'1\c' = 'c'</VERB> are performed.</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
ldivf('1','1')
ldivf('a','0')
ldivf('a','x')
ldivf('2','4')
 ]]>
  </EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>rdivf</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>addf</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>mulf</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>evstr</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>
