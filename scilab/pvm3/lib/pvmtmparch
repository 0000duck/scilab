#!/bin/sh
#
# $Id: pvmtmparch,v 1.1 2001/04/26 07:47:08 scilab Exp $
#
# pvmtmparch.sh
#
# Generate temporary PVM architecture string.
#
# First check for existing env var $PVM_ARCH.
#
# If not set, then just call "pvmgetarch".
#

if [ "#$PVM_ARCH#" != "##" ]; then
	echo "$PVM_ARCH"
else
	here="`dirname $0`"
	echo "`$here/pvmgetarch`"
fi

exit

