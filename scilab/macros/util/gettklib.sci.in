function tklib=gettklib()
  if MSDOS then 
    tklib='tk84.dll'
  else 
	execstr('link(''@GETLIBTK@'')', 'errcatch')
	if ans == 0 then
	    tklib='@GETLIBTK@'
	else
		execstr('link(''@GETLIBTK@.0'')', 'errcatch')
		if ans == 0 then
		    tklib='@GETLIBTK@.0'
		else
			 mprintf('Warning: Error loading @GETLIBTK@ :""'+lasterror()+'""')
		end
	end
  end
endfunction
