<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Fortran to C conversion"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="f2c rule"
			DisplayName="f2c rule"
			CommandLine="&quot;$(SolutionDir)bin\f2c.exe&quot; -I&quot;$(SolutionDir)modules\core\includes&quot; &quot;$(InputDir)$(InputFileName)&quot;"
			Outputs="&quot;$(InputDir)$(InputName).c&quot;"
			FileExtensions="*.f"
			ExecutionDescription="Converting $(InputName).f to $(InputName).c ..."
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
