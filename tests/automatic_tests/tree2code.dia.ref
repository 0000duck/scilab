 
getf SCI/util/testexamples.sci
 
reinit_for_test()
 
%U=mopen('SCI/tests/automatic_tests/tree2code_data.ref','r');
 
tree = macr2tree(help);
 
txt = tree2code(tree, %T);
 
%ans = write(%io(2), txt, '(a)');
function [] = help(key)
//for compatibility with toolboxes making use of old cat files
change_old_man
INDEX = make_help_index()
 
if argn(2)==0 then
  browsehelp(INDEX,"index");
  return;
end;
 
key = stripblanks(key)
if or(part(key,1)==["(",")","[","]","{","}","%","''","""",:,"*","/","\",".","<",">","&","^","|","~","+","-"]) then
  key = "symbols";
end;
path = gethelpfile(key)
if path~=[] then
  browsehelp(path,key)
else
  apropos(key)
end;
 
endfunction
 
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
xdel_run(winsid());
 
 
mclose(%U);
 
